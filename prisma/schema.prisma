generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlserver"
  url      = env("DATABASE_URL")
}

model User {
  id    Int    @id @default(autoincrement())
  name  String @db.NVarChar(50)
  email String @unique @db.VarChar(100)
  password String @db.NVarChar(1024)
}
